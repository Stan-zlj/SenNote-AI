
{
  "name": "zen-note-ai",
  "version": "1.0.0",
  "description": "AI-powered floating study companion",
  "author": "Stan",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "start": "electron .",
    "build": "vite build && electron-builder",
    "build:fast": "vite build && electron-builder --win dir"
  },
  "devDependencies": {
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "vite": "^5.4.11",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.0",
    "@google/genai": "^0.21.0"
  },
  "build": {
    "appId": "com.zennote.ai",
    "productName": "ZenNoteAI",
    "copyright": "Copyright Â© 2024 Stan",
    "directories": {
      "output": "dist_electron"
    },
    "electronDownload": {
      "mirror": "https://npmmirror.com/mirrors/electron/"
    },
    "win": {
      "target": [
        "dir",
        "portable"
      ],
      "icon": "https://cdn-icons-png.flaticon.com/512/566/566437.png"
    },
    "files": [
      "dist/**/*",
      "main.js"
    ],
    "publish": null
  }
}
